
config sysmonitor 'sysmonitor'
	option enable '1'
	option ddns '0'
	option gateway '192.168.1.1'
	option vpnip '192.168.1.110'
	option smartdns '1'
	option smartdnsPORT '53'
	option smartdnsAD '0'
	option samba '1'
	option samba_rw '0'
	option samba_rw_dir 'app,aria2'
	option nfs '1'
	option nfs_rw '0'
	option nfs_rw_dir 'app,aria2'
	option ftp '1'
	option minidlna '1'
	option minidlna_dir 'A-music,V-video'
	option vpns '0'
	option vpnp '0'

